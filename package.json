{
  "name": "amplify",
  "version": "1.0.0",
  "main": "index.js",
  "author": "groman00",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/EarTickling/amplify.git"
  },
  "scripts": {
    "dev": "NODE_ENV=development webpack --progress --hide-modules --config=./build/webpack.dev.conf",
    "watch": "NODE_ENV=development webpack --progress --watch --hide-modules --config=./build/webpack.dev.conf",
    "hot": "NODE_ENV=development webpack-dev-server --config=./build/webpack.dev.conf",
    "prod": "NODE_ENV=production webpack --config=./build/webpack.prod.conf",
    "server": "node ./server.js",
    "deploy": "yarn && yarn prod && forever start -d ./forever.json",
    "stop": "forever stopall"
  },
  "dependencies": {
    "axios": "^0.17.0",
    "base64-arraybuffer": "^0.1.5",
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "vue": "^2.5.3",
    "vue-material": "^0.8.1",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "copy-webpack-plugin": "^4.2.0",
    "css-loader": "^0.28.7",
    "dotenv": "^4.0.0",
    "dotenv-webpack": "^1.5.4",
    "forever": "^0.15.3",
    "node-sass": "^4.8.3",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.19.0",
    "sw-precache-webpack-plugin": "^0.11.4",
    "sw-toolbox": "^3.6.0",
    "uglifyjs-webpack-plugin": "^1.1.4",
    "vue-loader": "^13.4.0",
    "vue-template-compiler": "^2.5.3",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4",
    "webpack-merge": "^4.1.1"
  }
}
